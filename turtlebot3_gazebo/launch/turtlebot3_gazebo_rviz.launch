<launch>
  <arg name="model" default="burger"/>
  <arg name="multi_robot_name" default=""/>

  <param name="robot_description" command="$(find xacro)/xacro --inorder $(find omni_turtlebot3_description)/urdf/turtlebot3_$(arg model).urdf.xacro" />

  <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
    <param name="publish_frequency" type="double" value="50.0" />
    <param name="tf_prefix" value="$(arg multi_robot_name)"/>
  </node>

  <!-- include file="$(find turtlebot3_bringup)/launch/turtlebot3_remote.launch">
    <arg name="model" value="$(arg model)"/>
  </include -->

  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find omni_turtlebot3_description)/rviz/model.rviz"/>
</launch>




